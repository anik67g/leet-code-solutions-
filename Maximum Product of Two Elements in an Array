class Solution {
public:
    int maxProduct(vector<int>& nums) {
        int maxi=INT_MIN;
        int secmaxi=INT_MIN;
        for(int i=0;i<nums.size();i++){
            if(maxi<nums[i]){
                secmaxi=maxi;
                maxi=nums[i];
            }
            else if(secmaxi<nums[i] ){
                secmaxi=nums[i];
            }
        }

        return (maxi-1)*(secmaxi-1);
    }
};
